{
  "name": "wechat-helper",
  "version": "1.0.10",
  "main": "lib/index",
  "scripts": {
    "dev": "cross-env NODE_ENV=dev SERVER_ENV=dev webpack -w -p --progress --colors &  node webserver.js",
    "start": "cross-env NODE_ENV=dev SERVER_ENV=dev webpack&&webpack-dev-server -d",
    "build": "cross-env NODE_ENV=production SERVER_ENV=prod webpack -p --progress --colors --mode production",
    "buildesm": "cross-env NODE_ENV=production SERVER_ENV=prod webpack -p --progress --colors --mode production --config webpack.esm.config.js",
    "build:dev": "cross-env NODE_ENV=dev SERVER_ENV=dev webpack -p --progress --colors",
    "buildesm:dev": "cross-env NODE_ENV=dev SERVER_ENV=dev webpack -p --progress --colors --config webpack.esm.config.js",
    "gulp": "gulp",
    "report": "cross-env NODE_ENV=production npm_config_report=true yarn run build",
    "layout": "webpack --progress --mode production --config webpack.layout.conf.js",
    "dllcss": "cross-env NODE_ENV=production webpack -p --progress --colors --mode production --config webpack.dllcss.js",
    "dll": "cross-env NODE_ENV=production webpack -p --progress --colors --mode production --config webpack.dll.js"
  },
  "files": [
    "lib",
    "es",
    "css",
    "demo"
  ],
  "license": "MIT",
  "theme": {
    "@fill-body": "#f6f6f6",
    "@brand-primary": "#ff8040",
    "@brand-primary-tap": "#fc7634",
    "@color-text-base": "#333333",
    "@font-size-heading": "14px",
    "@font-size-caption": "14px",
    "@input-font-size": "15px",
    "@color-text-caption": "#666666",
    "@fill-tap": "#efefef"
  },
  "dependencies": {
    "@babel/core": "7.7.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.8.3",
    "@babel/plugin-proposal-private-methods": "^7.8.3",
    "@babel/plugin-transform-runtime": "7.5.0",
    "@babel/polyfill": "^7.8.7",
    "@babel/preset-env": "7.7.7",
    "@babel/preset-react": "^7.8.3",
    "@babel/runtime-corejs3": "^7.8.4",
    "@purtuga/esm-webpack-plugin": "^1.2.1",
    "axios": "^0.20.0",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "^7.0.2",
    "css-loader": "^3.4.2",
    "file-loader": "^3.0.1",
    "hard-source-webpack-plugin": "^0.13.1",
    "html-webpack-plugin": "^4.0.3",
    "mini-css-extract-plugin": "^0.9.0",
    "minimist": "^1.2.4",
    "node-sass": "^4.14.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-cssnext": "^3.1.0",
    "postcss-loader": "^3.0.0",
    "qs": "^6.9.1",
    "sass-loader": "^8.0.2",
    "speed-measure-webpack-plugin": "^1.3.3",
    "style-loader": "^1.1.3",
    "swiper": "5.4.5",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-loader": "^3.0.0",
    "webpack": "^4.42.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "babel-plugin-import": "^1.13.0",
    "browser-sync": "^2.26.7",
    "bundle-loader": "^0.5.2",
    "eslint": "^7.4.0",
    "eslint-config-alloy": "^3.7.3",
    "eslint-plugin-react": "^7.20.3",
    "webpack-bundle-analyzer": "^3.6.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-visualizer-plugin": "^0.1.11"
  },
  "author": "changx",
  "engines": {
    "node": ">=10.18.0",
    "npm": ">=6.13.4"
  }
}
